점수들 = [90, 25, 67, 45, 80, 88, 98,11]

number = 0
for 점수 in 점수들:
    number = number + 1
    if 점수 >= 60:
        print("{}번 학생은 합격입니다.".format(number))
    else:
        print("{}번 학생은 불합격입니다.".format(number))


