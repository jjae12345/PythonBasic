print(수학.더하기(3,4))

print(수학.빼기(4,2))

from 수학 import 더하기, 빼기
print(더하기(3, 4))

import time

print('현재시각:', time.time())

def manyloop(max):
    t1 = time.time()
    for a in range(max):
        pass
    t2 = time.time()
    print(t2-t1, '초 경과')

number = int(input('숫자를 입력하세요: '))
manyloop(number)
